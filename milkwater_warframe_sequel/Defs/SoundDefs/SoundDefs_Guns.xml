<Defs>
	<!--=============== Gorgon ====================-->
    <SoundDef>
        <defName>Milkwater_Shot_Gorgon</defName>
        <context>MapOnly</context>
        <eventNames/>
        <maxSimultaneous>1</maxSimultaneous>
        <subSounds>
            <li>
                <grains>
                    <li Class="AudioGrain_Clip">
                        <clipPath>Weapons/Milkwater_Shot_Gorgon</clipPath>
