<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<HediffDef ParentName="RoleStatBuff">
		<defName>WF_Generic_PsyfocusRegen</defName>
		<label>natural psyfocus regeneration</label>
		<description>This pawn will naturally regenerate psyfocus every so often without the need to meditate.</description>
		<hediffClass>WarframeModSequel.WarframePsyfocusRegenerationHediff</hediffClass>
		<defaultLabelColor>(0.4, 1, 0.7)</defaultLabelColor>
		<isBad>False</isBad>
</HediffDef>

<HediffDef ParentName="RoleStatBuff">
		<defName>WF_Generic_NaturalArmor</defName>
		<label>natural armor</label>
		<description>This pawn has a naturally armored body.</description>
		<defaultLabelColor>(0.8, 0.8, 0.8)</defaultLabelColor>
		<isBad>False</isBad>
		<stages>
		  <li>
			<statOffsets>
			  <ArmorRating_Sharp>0.50</ArmorRating_Sharp>
			  <ArmorRating_Blunt>0.50</ArmorRating_Blunt>
			</statOffsets>
		  </li>
        </stages>
</HediffDef>

</Defs>